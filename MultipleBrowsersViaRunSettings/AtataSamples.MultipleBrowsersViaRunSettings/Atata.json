{
  "drivers": [
    {
      "type": "chrome",
      "alias": "chrome",
      "options": {
        "arguments": [
          "start-maximized",
          "disable-search-engine-choice-screen"
        ]
      }
    },
    {
      "type": "chrome",
      "alias": "chrome-headless",
      "options": {
        "arguments": [
          "headless=new",
          "window-size=1920,1080",
          "disable-search-engine-choice-screen"
        ]
      }
    },
    {
      "type": "firefox"
    }
  ],
  "baseUrl": "https://demo.atata.io/",
  "culture": "en-US",
  "useAllNUnitFeatures": true
}
